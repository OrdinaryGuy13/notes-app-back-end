{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-unused-vars":"off",
        "comma-dangle":"off",
        "no-console":"off",
        "indent":"off",
        "no-trailing-spaces": "off",
        "object-curly-newline": "off"
    }
}
